---
import LinkItem from "../LinkItem.astro"

interface Props {
    link?: string | null
    img?: string | null
    text?: string | null
}

const { img = null, link = null, text = null } = Astro.props
---

<LinkItem
    link={link ?? "https://vitejs.dev/"}
    img={img ?? "vite.png"}
    text={text ?? "Vite"}
/>

---
import LinkItem from "../LinkItem.astro"

interface Props {
    link?: string | null
    img?: string | null
    text?: string | null
}

const { img = null, link = null, text = null } = Astro.props
---

<LinkItem
    link={link ?? "https://ui.shadcn.com/"}
    img={img ?? "shad-cn.png"}
    text={text ?? "Shad-cn"}
/>
